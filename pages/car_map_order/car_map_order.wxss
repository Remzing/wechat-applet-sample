/* pages/homepage/index.wxss */
page{
  /* height:100vh;  */
  display: flex;
  flex-direction: column;
}
.map-box{
  width: 100%;
  height: calc( 100% - 400rpx );
  overflow: hidden;
}
#myMap{
  width: 100%;
  height: 100%; 
}
.main-btn{
  background:#fff;
  padding:20rpx;
}
.main-text{
  display:flex;
  justify-content:center;
  align-items:center;
  text-align:center;
  height:100rpx;
  margin:0 auto;
  color:#fff;
  background:#4a4c5b;
  font-size:32rpx;
  border-radius:4rpx;
}
cover-image{
