  flex-direction: row;
  background: #fff;
  align-items: center;
  padding-left: 20rpx;
  font-size: 28rpx;
  color: #666;
}
.top-icon{
  position:static;
  width: 74rpx;
  height: 74rpx;
  margin-right: 24rpx;
  border-radius: 50%;
}
.top-text{
  position:static;
